{
	"name": "Vimtips Plus",
	"description": "Vim tips with a built-in wasm implemention of vim. Learn vim with daily small tips that you can practice right in the browser.",
	"version": "1.0.0",
	"manifest_version": 3,
	"incognito": "split",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": [
		"storage"
	],
	"background": {
		"service_worker": "assets/vim-CTwK2WCj.js",
		"type": "module"
	},
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"sandbox": {
		"pages": [
		  "terminal-De0kndZN.html"
		]
	}
}