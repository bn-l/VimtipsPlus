{
    "name": "Vimtips Plus",
    "description": "Learn vim with daily small tips that you can practice right in the browser with this new tab extension. Features a built in-vim terminal implemented in wasm.",
    "version": "1.0.6",
    "manifest_version": 3,
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "offscreen"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';",
        "sandbox": "sandbox allow-scripts 'unsafe-inline' 'unsafe-eval';"
    },
    "sandbox": {
        "pages": [
            "VimTerminal/terminal.html"
        ]
    }
}